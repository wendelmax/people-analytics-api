{
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 100,
  "tabWidth": 2,
  "semi": true,
  "arrowParens": "always",
  "bracketSpacing": true,
  "endOfLine": "auto",
  "importOrder": [
    "^@nestjs/(.*)$",
    "^@prisma/(.*)$",
    "^@core/(.*)$",
    "^@common/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}