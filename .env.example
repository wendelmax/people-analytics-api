# Database
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/people_analytics?schema=public"

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# JWT
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRATION=1d

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=logs/
LOG_MAX_SIZE=10m
LOG_MAX_FILES=7

# Rate Limiting
RATE_LIMIT_POINTS=10
RATE_LIMIT_DURATION=1

# Server
PORT=3000
NODE_ENV=development
